{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "functions": {
    "api/*.ts": {
      "memory": 1024
    }
  },
  "rewrites": [
    { "source": "/api/notion/status", "destination": "/api/notion-status" },
    { "source": "/api/notion/databases", "destination": "/api/notion-databases" },
    { "source": "/api/notion/setup", "destination": "/api/notion-setup" },
    { "source": "/api/notion/sample-data", "destination": "/api/sample-data" },
    { "source": "/api/airdrops/featured", "destination": "/api/airdrops-featured" },
    { "source": "/api/airdrops", "destination": "/api/airdrops" }
  ]
}