{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "functions": {
    "api/*.ts": {
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/notion/status", "dest": "/api/notion-status" },
    { "src": "/api/notion/databases", "dest": "/api/notion-databases" },
    { "src": "/api/notion/setup", "dest": "/api/notion-setup" },
    { "src": "/api/notion/sample-data", "dest": "/api/sample-data" },
    { "src": "/api/airdrops/featured", "dest": "/api/airdrops-featured" },
    { "src": "/api/airdrops", "dest": "/api/airdrops" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.+\\.[a-zA-Z]+)$", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "outputDirectory": "dist/public"
}